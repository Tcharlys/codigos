Algoritmo "LISTA III - Q3"
Var

n: inteiro

Inicio
      escreval("Digite um número inteiro")
      leia(n)
      
      se n > 0 entao
         escrevaL("O Nº informado é POSITIVO")
      senao
           se n < 0 entao
              escreval("O Nº é NEGATIVO")

              senao
                   escreval("O Nº é NEUTRO")
           fimse
      fimse
      
Fimalgoritmo